 let  persons:any[] = [{ "name": "Tony", "checked": false },

        { "name": "Stark", "checked": false },

        { "name": "Mark", "checked": false },

        { "name": "Henry", "checked": false }

    ];

  console.log("Persons are: ");

 for(let i=0;i< persons.length;i++){

     console.log(persons[i].name);

 }
